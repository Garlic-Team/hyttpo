{
  "name": "hyttpo",
  "version": "0.2.8",
  "description": "Easy way to send requests for node.js and browser",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.cjs"
  },
  "homepage": "https://garlic-team.github.io/hyttpo",
  "bugs": {
    "url": "https://github.com/Garlic-Team/hyttpo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/garlic-team/hyttpo.git"
  },
  "author": "Garlic Team",
  "license": "ISC",
  "scripts": {
    "build": "bash build.bash",
    "build:rollup": "rollup --config --sourcemap",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "build:webpack": "npx webpack",
    "docs:build": "typedoc --json docs/out-main.json src/index.ts && node scripts/docs.mjs"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@discordjs/ts-docgen": "^0.3.4",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/eslint": "^8.2.1",
    "@types/estree": "^0.0.50",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "assert": "^2.0.0",
    "babel-cli": "^6.26.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "eslint": "^8.4.1",
    "https-browserify": "^1.0.0",
    "process": "^0.11.10",
    "rollup": "^2.61.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "^4.4.3",
    "url": "^0.11.0",
    "util": "^0.12.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "follow-redirects": "^1.14.6"
  }
}
