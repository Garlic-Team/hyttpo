    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "assert": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "eslint": "^8.4.1",
    "https-browserify": "^1.0.0",
    "process": "^0.11.10",
    "rollup": "^2.61.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
